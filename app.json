{
  "expo": {
    "name": "Anime Finder",
    "description": "This app helps you find anime through images",
    "slug": "expo-anime-finder",
    "version": "1.0.0",
    "githubUrl": "https://github.com/carlos3g/expo-anime-finder",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#1E1E2C",
    "userInterfaceStyle": "dark",

    "assetBundlePatterns": ["**/*"],
    "updates": {
      "enabled": false
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E1E2C"
    },
    "androidNavigationBar": {
      "visible": "sticky-immersive",
      "barStyle": "light-content",
      "backgroundColor": "#1E1E2C"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "translucent": true
    },
    "android": {
      "enableDangerousExperimentalLeanBuilds": true,
      "permissions": ["CAMERA", "MEDIA_LIBRARY"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1E1E2C"
      }
    }
  }
}
